{
  "name": "remix-boilerplate",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "rm -rf build 2>&1; remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "dotenv -e .env.production remix-serve ./build/server/index.js",
    "zen": "zenstack generate && dotenv -- prisma db push",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@clerk/localizations": "^2.0.0-beta.17",
    "@clerk/remix": "^3.2.0",
    "@conform-to/react": "^1.0.6",
    "@conform-to/zod": "^1.0.6",
    "@prisma/client": "^5.11.0",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@tailwindcss/typography": "^0.5.10",
    "@zenstackhq/runtime": "1.11.1",
    "daisyui": "^4.9.0",
    "isbot": "^5.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.8.1",
    "@types/react": "^18.2.71",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "autoprefixer": "^10.4.19",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "hygen": "^6.2.11",
    "prisma": "^5.11.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.6",
    "vite-tsconfig-paths": "^4.3.2",
    "zenstack": "1.11.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
