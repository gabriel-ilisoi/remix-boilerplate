{
  "name": "remix-boilerplate",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "rm -rf build 2>&1; remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "dotenv -e .env.production remix-serve ./build/server/index.js",
    "zen": "zenstack generate && dotenv -- prisma db push",
    "typecheck": "tsc",
    "plop": "md5sum --check --status plopfile.md5 || npm run plop:transpile && plop",
    "plop:transpile": "echo 'Deleting plopfile and regenerate it+md5' && rimraf ./plopfile.js && tsup ./plopfile.ts --format esm --out-dir . && md5sum plopfile.ts > plopfile.md5"
  },
  "dependencies": {
    "@clerk/localizations": "^2.0.0-beta.17",
    "@clerk/remix": "^3.2.0",
    "@prisma/client": "^5.11.0",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@tailwindcss/typography": "^0.5.10",
    "@zenstackhq/runtime": "1.10.3",
    "daisyui": "^4.7.3",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.8.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.18",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "hygen": "^6.2.11",
    "prisma": "^5.11.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.2",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "zenstack": "1.10.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
